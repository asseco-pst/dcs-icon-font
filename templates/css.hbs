@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

.{{baseSelector}} {
	line-height: normal;
	font-size: 2em;
}

.{{baseSelector}}:before {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	vertical-align: top;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}

.{{baseSelector}}.{{classPrefix}}sm {
  font-size: 1em;
}
.{{baseSelector}}.{{classPrefix}}1-5x {
  font-size: 1.5em;
}
.{{baseSelector}}.{{classPrefix}}2x {
  font-size: 2em;
}
.{{baseSelector}}.{{classPrefix}}3x {
  font-size: 3em;
}
.{{baseSelector}}.{{classPrefix}}4x {
  font-size: 4em;
}
